{% extends 'base.html.twig' %}
{% trans_default_domain 'FOSUserBundle' %}

{% block body %}

<table class="table" style="margin-top: -25%;">
  <thead class="thead-light">
    <tr>
      <th scope="col">{{ 'profile.show.username'|trans }}</th>
      <th scope="col">{{ 'profile.show.email'|trans }}</th>
    </tr>
  </thead>
<tbody>
    <tr>
     <td>{{ user.username }}</td>
     <td>{{ user.email }}</td>
    </tr>
</tbody>
</table>

{% endblock %}